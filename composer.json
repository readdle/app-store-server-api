	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Pavlo Kotets",
			"email": "pkotets@readdle.com",
			"homepage": "https://github.com/pkotets"
		}
	],
	"keywords": [
		"app store",
		"server api",
		"server notifications",
		"itunes",
		"apple",
		"in app",
		"purchase",
		"parser",
		"server-to-server",
		"php"
	],
	"homepage": "https://github.com/readdle/app-store-server-api",
	"version": "3.4.1",
	"php": ">=7.4",
	"autoload": {
		"psr-4": {
			"Readdle\\AppStoreServerAPI\\": "src/"
